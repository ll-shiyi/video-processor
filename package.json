{
  "name": "video-processor",
  "version": "1.0.0",
  "description": "视频处理器 - 基于 pose-detection 的人体姿态识别和视频处理工具",
  "main": "processor.js",
  "scripts": {
    "exec:fast": "node exec.js FAST_720P",
    "exec:ultra": "node exec.js ULTRA_FAST",
    "exec:standard": "node exec.js STANDARD",
    "exec:quality": "node exec.js HIGH_QUALITY",
    "vod:eye-blur": "node vod_eye_blur_example.js",
    "vod:optimized": "node vod_eye_blur_optimized_example.js",
    "vod:optimized:test": "node vod_eye_blur_optimized_example.js --test",
    "vod:fast": "node vod_eye_blur_fast_example.js",
    "vod:fast:test": "node vod_eye_blur_fast_example.js --test",
    "test:balanced": "node test_balanced_mode.js",
    "simple:balanced": "node simple_balanced_test.js",
    "debug:processor": "node processor_debug.js --width 1280 --height 720 --showProgress true --saveNoFaceFrames true --saveAllFrames true"
